{
  "": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "nixpacks",
    "config": {
      "nixpacks": {
        "resources": {
          "memory": 512
        }
      }
    }
  },
  "deploy": {
    "startCommand": "node src/app.js",
    "healthcheckPath": "/health",
    "healthcheckTimeout": 5,
    "healthcheckStartPeriod": 20
  }
}
